{
  "name": "run-callback",
  "version": "2.1.0",
  "description": "Run async & sync callbacks",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run lint && task-tape test/*.js | tap-summary",
    "lint": "eslint *.js lib/**/*.js test/*.js bin/*.js",
    "coverage": "istanbul cover -i 'lib/**/*.js' -i '*.js' --print both task-tape -- test/*.js",
    "check-coverage": "istanbul check-coverage --statements 90 --functions 90 --branches 85 --lines 90",
    "upload-coverage": "cat ./coverage/lcov.info | coveralls",
    "cover": "npm run coverage && npm run check-coverage && npm run upload-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zoubin/run-callback.git"
  },
  "keywords": [
    "async",
    "callback"
  ],
  "author": "zoubin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zoubin/run-callback/issues"
  },
  "homepage": "https://github.com/zoubin/run-callback#readme",
  "dependencies": {
    "end-of-stream": "^1.1.0",
    "once": "^1.3.2",
    "stream-consume": "^0.1.0"
  },
  "devDependencies": {
    "coveralls": "^2.11.4",
    "eslint": "^1.4.3",
    "istanbul": "^0.4.0",
    "tap-summary": "^1.0.0",
    "tape": "^4.2.0",
    "task-tape": "^1.2.2"
  }
}
